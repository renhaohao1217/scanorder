/* navigationBar.wxss */
.navigation-container {
  position: fixed;
  width: 100%;
  z-index: 99;
  top: 0;
  left: 0;
  background-color: rgb(245, 245, 245);
}
.navigation-bar {
  position: relative;
  display: flex;
  flex-direction: row;
  align-items: center;
}
.navigation-buttons {
  display: flex;
  align-items: center;
  justify-content: center;
  margin-left: 10px;
  box-sizing: border-box;
  background-color: transparent;
  z-index: 100;
}
.nav-img {
  height: 16px;
  width: 16px;
}
.navigation-title {
  position: absolute;
  left: 0;
  right: 0;
  text-align: center;
  font-size: 16px;
  color: #000000;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
